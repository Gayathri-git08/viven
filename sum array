#include <stdio.h>
int main()
{
    int i,arr[500],size,sum=0;
    printf("Enter the size of the array: ");
    scanf("%d",&size);
    printf("Enter the %d array elements: ",size);
    for(i=0;i<size;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<size;i++){
        sum+=arr[i];
    }
    printf("Sum of the given elements are: %d",sum);
}
