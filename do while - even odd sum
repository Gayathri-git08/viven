#include <stdio.h>
int main()
{
    int i=1,n,num,evensum=0,oddsum=0;
    printf("Enter the range: ");
    scanf("%d",&n);
    printf("Enter the numbers: ");
    scanf("%d",&num);
    do{
        if(i%2==0){
            evensum+=i;
        }
        else{
            oddsum+=i;
        }
        i++;
    }while(i<=n);
    printf("Sum of even numbers: %d\n",evensum);
    printf("Sum of odd numbers: %d",oddsum);
}
