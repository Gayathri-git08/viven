#include <stdio.h>

int main()
{
    int i,n,arr[50],Ceven=0,Codd=0;
    printf("Enter the size of the array: ");
    scanf("%d",&n);
    printf("Enter the %d array elements in the array: ",n);
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++){
        if(arr[i]%2==0){
            Ceven++;
        }
        else
        Codd++;
    }
    printf("The even elements are: %d\n",Ceven);
    printf("The odd elements are: %d\n",Codd);
}
